<?xml version="1.0"?>
 
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
 
    <Description about="urn:mozilla:install-manifest">
	<em:id>Artem@Demchenkov.ImageSpider</em:id>
	<em:version>2.0.2</em:version>
	<em:type>2</em:type>
 
    <!-- Target Application this extension can install into,
         with minimum and maximum supported versions. -->
	<em:targetApplication>
	    <Description>
		<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
		<em:minVersion>3.0a1</em:minVersion>
		<em:maxVersion>19.0a1</em:maxVersion>
	    </Description>
	</em:targetApplication>
 
    <!-- Front End MetaData -->
	<em:name>Image Spider</em:name>
	<em:description>This extension shows the image parameters, if you move cursor under it</em:description>
	<em:creator>Artem Demchenkov</em:creator>
	<em:iconURL>chrome://imageparams/skin/icon.png</em:iconURL>
	<em:aboutURL>chrome://imageparams/content/about/about.xul</em:aboutURL>
	<em:optionsURL>chrome://imageparams/content/settings/settings.xul</em:optionsURL>
    </Description>     
</RDF>